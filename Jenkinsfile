#jenkins pipeline script
#pipeline with stages compile, test, build and echo message in the stages

pipeline {
    agent any
    stages {
        stage('Compile Stage') {
            steps {
                echo 'Compiling the code'
            }
        }
        stage('Test Stage') {
            steps {
                echo 'Testing the code'
            }
        }
        stage('Build Stage') {
            steps {
                echo 'Building the code'
            }
        }
    }
}